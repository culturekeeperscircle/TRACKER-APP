async function handleAiQuery() {
  const query = document.getElementById('aiQuery').value.trim();
  if (!query) return;
  
  const btn = document.getElementById('aiBtn');
  const responseDiv = document.getElementById('aiResponse');
  const responseText = document.getElementById('aiResponseText');
  
  btn.textContent = '‚è≥...';
  btn.disabled = true;
  responseDiv.style.display = 'block';
  responseText.textContent = 'Processing...';
  
  const allDocs = getAllItems();
  const stats = {
    total: allDocs.length,
    byCategory: DATA.meta.by_category,
    critical: allDocs.filter(d => d.L === 'CRITICAL').length,
    moderate: allDocs.filter(d => d.L === 'MODERATE').length,
    protective: allDocs.filter(d => d.L === 'PROTECTIVE').length
  };
  
  const queryLower = query.toLowerCase();
  const relevantDocs = allDocs.filter(d => {
    const text = [d.T, d.s, d.D, d.I, ...(d.A || []), ...(d.c || [])].join(' ').toLowerCase();
    return queryLower.split(' ').some(w => w.length > 3 && text.includes(w));
  }).slice(0, 20).map(d => ({
    title: d.s || d.T, date: d.d, type: d.t, category: d._category,
    agency: (d.A || []).join(', '), threatLevel: d.L, summary: (d.D || '').substring(0, 250)
  }));
  
  const systemPrompt = `You are an expert assistant for the TCKC Threat Tracker database. The database is organized by ACTION TYPE:
- Executive Orders: ${stats.byCategory.executive_orders} entries (Presidential directives, proclamations)
- Agency Actions: ${stats.byCategory.agency_actions} entries (Federal agency rules, orders, budgets, personnel)
- Legislation: ${stats.byCategory.legislation} entries (Public laws, bills)
- Litigation: ${stats.byCategory.litigation} entries (Federal lawsuits, court outcomes)
- International: ${stats.byCategory.international} entries

THREAT STATISTICS:
- Total: ${stats.total} | CRITICAL: ${stats.critical} | MODERATE: ${stats.moderate} | PROTECTIVE: ${stats.protective}

RELEVANT DOCUMENTS (${relevantDocs.length} found):
${JSON.stringify(relevantDocs, null, 2)}

Be specific about categories, agencies, dates, and threat levels.`;

  try {
    const response = await fetch("https://api.anthropic.com/v1/messages", {
      method: "POST",
      headers: { "Content-Type": "application/json" },
      body: JSON.stringify({
        model: "claude-sonnet-4-20250514",
        max_tokens: 1500,
        system: systemPrompt,
        messages: [{ role: "user", content: query }]
      })
    });
    const data = await response.json();
    responseText.textContent = data.content?.map(item => item.text || '').join('\n') || 'No response.';
  } catch (e) {
    responseText.textContent = 'Error processing query.';
  } finally {
    btn.textContent = 'üîç Ask';
    btn.disabled = false;
  }
}

document.addEventListener('click', (e) => {
  if (!e.target.closest('.multi-select')) {
    document.querySelectorAll('.multi-select-dropdown').forEach(d => d.classList.remove('show'));
    document.querySelectorAll('.multi-select-btn').forEach(b => b.classList.remove('open'));
  }
});

document.querySelectorAll('.multi-select-option').forEach(opt => {
  opt.addEventListener('click', function(e) {
    e.stopPropagation();
    const value = this.dataset.value;
    const multiSelect = this.closest('.multi-select');
    const filterType = multiSelect.dataset.filter;
    const checkbox = this.querySelector('input');
    
    if (value === '') {
      filters[filterType] = [];
      multiSelect.querySelectorAll('.multi-select-option').forEach(o => {
        o.classList.remove('selected');
        o.querySelector('input').checked = o.dataset.value === '';
      });
      this.classList.add('selected');
    } else {
      const allOption = multiSelect.querySelector('[data-value=""]');
      allOption.classList.remove('selected');
      allOption.querySelector('input').checked = false;
      
      if (filters[filterType].includes(value)) {
        filters[filterType] = filters[filterType].filter(v => v !== value);
        this.classList.remove('selected');
        checkbox.checked = false;
      } else {
        filters[filterType].push(value);
        this.classList.add('selected');
        checkbox.checked = true;
      }
      
      if (filters[filterType].length === 0) {
        allOption.classList.add('selected');
        allOption.querySelector('input').checked = true;
      }
    }
    
    const btn = multiSelect.querySelector('.multi-select-btn');
    const selected = filters[filterType];
    btn.textContent = selected.length === 0 ? 'All' : selected.length === 1 ? selected[0] : selected.length + ' sel';
    btn.classList.toggle('has-selection', selected.length > 0);
    filterCards();
  });
});

