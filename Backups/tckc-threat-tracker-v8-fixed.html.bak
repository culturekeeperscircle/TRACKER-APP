function renderImpact(impact) {
  if (!impact) return '';
  const items = [];
  if (impact.people) items.push({key:'people',icon:'üë•',label:'PEOPLE',data:impact.people});
  if (impact.places) items.push({key:'places',icon:'üìç',label:'PLACES',data:impact.places});
  if (impact.practices) items.push({key:'practices',icon:'üé®',label:'PRACTICES',data:impact.practices});
  if (impact.treasures) items.push({key:'treasures',icon:'üíé',label:'TREASURES',data:impact.treasures});
  if (items.length === 0) return '';
  
  const cards = items.map(item => {
