function matchesCommunity(item, cf) {
  if (cf.length === 0) return true;
  const allText = [item.T, item.D, item.I, ...(item.c || [])].join(' ');
  return cf.some(f => (COMMUNITY_KEYWORDS[f] || []).some(kw => allText.toLowerCase().includes(kw.toLowerCase())));
}

function matchesResource(item, rf) {
  if (rf.length === 0) return true;
  const impact = item.I;
  if (!impact || typeof impact !== 'object') return false;
  return rf.some(f => impact[f] && (impact[f].n || impact[f].d));
}



// Legislation Progress Bar
const LEG_STAGES = [
  { id: 'intro', label: 'Intro', pct: 10, width: '10%' },
  { id: 'committee', label: 'Committee', pct: 25, width: '15%' },
  { id: 'markup', label: 'Markup', pct: 35, width: '10%' },
  { id: 'floor', label: 'Floor Vote', pct: 50, width: '15%' },
  { id: 'chamber2', label: '2nd Chamber', pct: 65, width: '15%' },
  { id: 'conference', label: 'Conference', pct: 75, width: '10%' },
  { id: 'final', label: 'Final Vote', pct: 85, width: '10%' },
  { id: 'president', label: 'President', pct: 100, width: '15%' }
];

function mapStatusToProgress(status, docType) {
  if (!status) return 10;
  const s = (status + ' ' + (docType || '')).toLowerCase();
  
  // Enacted/Signed
  if (s.includes('public law') || s.includes('enacted') || s.includes('signed into law') || 
      s.includes('became law') || s.includes('pl ') || s.includes('p.l.')) return 100;
  
  // Final Vote / Resolving Differences
  if (s.includes('final vote') || s.includes('resolving differences') || 
      s.includes('sent to president') || s.includes('presented to president')) return 85;
  
  // Conference
  if (s.includes('conference') || s.includes('reconcil')) return 75;
  
  // Second Chamber (passed one chamber, in second)
  if (s.includes('passed house') && s.includes('senate') || 
      s.includes('passed senate') && s.includes('house') ||
      s.includes('received in senate') || s.includes('received in house') ||
      s.includes('second chamber')) return 65;
  
  // Floor Vote (passed one chamber)
  if (s.includes('passed house') || s.includes('passed senate') || 
      s.includes('agreed to') || s.includes('floor vote')) return 50;
  
  // Markup
  if (s.includes('markup') || s.includes('reported') || s.includes('ordered reported') ||
      s.includes('out of committee')) return 35;
  
  // Committee
  if (s.includes('committee') || s.includes('referred') || s.includes('hearing')) return 25;
  
  // Default to Introduced
  return 10;
}

function renderLegProgress(item) {
  const status = item.S || item.s || '';
  const docType = item.t || '';
  const progress = mapStatusToProgress(status, docType);
  const isEnacted = progress === 100;
  
  const stages = LEG_STAGES.map(stage => {
    let cls = 'leg-stage';
    if (progress >= stage.pct) {
      cls += isEnacted ? ' enacted' : ' completed';
      if (progress === stage.pct && !isEnacted) cls += ' current';
    }
    return `<div class="${cls}" style="width:${stage.width}">${stage.label}</div>`;
  }).join('');
  
  const statusText = isEnacted ? '‚úî Enacted into Law' : `Current: ${LEG_STAGES.find(s => s.pct >= progress)?.label || 'Intro'}`;
  const pctClass = isEnacted ? 'status-pct enacted' : 'status-pct';
  
  return `
    <div class="leg-progress-container">
      <div class="leg-progress-label">Legislative Progress</div>
      <div class="leg-progress-bar">${stages}</div>
      <div class="leg-progress-status">
        <span class="status-text">${statusText}</span>
        <span class="${pctClass}">${progress}%</span>
      </div>
    </div>
  `;
}


function filterCards() {
  const search = document.getElementById('search').value.toLowerCase();
  const sortOrder = document.getElementById('sortOrder').value;
  
  let items = currentCategory === 'all' ? getAllItems() : (DATA[currentCategory] || []).map(i => ({...i, _category: currentCategory}));
  
  items = items.filter(item => {
    if (search) {
      const searchable = [item.T, item.s, item.n, item.D, item.I, item.S, ...(item.A || []), ...(item.c || [])].join(' ').toLowerCase();
      if (!searchable.includes(search)) return false;
    }
    if (filters.threat.length > 0 && !filters.threat.includes(item.L)) return false;
    if (filters.year.length > 0 && !filters.year.includes(getYear(item.d))) return false;
    if (filters.admin.length > 0 && !filters.admin.some(f => (item.a || '').includes(f))) return false;
    if (filters.agency.length > 0 && !filters.agency.some(f => (item.A || []).some(a => a.includes(f)))) return false;
    if (filters.action.length > 0 && !filters.action.some(f => (item.t || '').includes(f))) return false;
    if (!matchesCommunity(item, filters.community)) return false;
    if (!matchesResource(item, filters.resource)) return false;
    
    const isAggregate = (item.T || '').toLowerCase().includes('aggregate') || (item.t || '').toLowerCase().includes('impact analy');
    if (showAggregateOnly && !isAggregate) return false;
    
    return true;
  });
  
  items.sort((a, b) => sortOrder === 'newest' ? (b.d || '').localeCompare(a.d || '') : (a.d || '').localeCompare(b.d || ''));
  
  renderCards(items);
  updateStats(items);
}

